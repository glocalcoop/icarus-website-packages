{
    "name": "glocalcoop/icarus-wp-content",
    "homepage": "https://github.com/glocalcoop/icarus-wp-content",
    "authors": [
        {
            "name": "Pea, Glocal",
            "email": "pea@glocal.coop"
        }
    ],
    "support": {
        "wiki": "https://plan.glocal.coop/projects/icarus/wiki"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "glocalcoop/icarus",
                "version": "master",
                "type": "wordpress-theme",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/glocalcoop/icarus-theme/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/glocalcoop/icarus-theme.git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "glocalcoop/icarus":"dev-master",
        "wpackagist-plugin/caldera-forms": ">=1.4.5",
        "wpackagist-plugin/event-organiser": ">=3.1dana.6",
        "wpackagist-plugin/event-organiser-csv": ">=0.3.2",
        "wpackagist-plugin/event-organiser-posterboard": ">=3.0.1",
        "wpackagist-plugin/jetpack": ">=4.4.1",
        "wpackagist-plugin/jetpack-module-control": ">=1.4.2",
        "wpackagist-plugin/yikes-inc-easy-mailchimp-extender": ">=6.2.3",
        "wpackagist-plugin/members": ">=1.1.2",
        "wpackagist-plugin/nav-menu-roles": ">=1.8.3",
        "wpackagist-plugin/wp-pagenavi": ">=2.91",
        "wpackagist-plugin/shortcode-ui": ">=0.7.0",
        "wpackagist-plugin/siteorigin-panels": ">=2.4.19",
        "wpackagist-plugin/so-widgets-bundle": ">=1.7.2",
    	"wpackagist-plugin/widgets-for-siteorigin": ">=1.2.2",
        "wpackagist-plugin/user-switching": ">=1.0.9",
        "wpackagist-plugin/widget-css-classes": ">=1.3.0",
        "wpackagist-plugin/wordpress-importer": ">=0.6.3",
        "wpackagist-plugin/wp-piwik": ">=1.0.12"
    },
    "require-dev": {
        "wpackagist-plugin/developer": ">=1.2.6",
        "wpackagist-plugin/which-template": ">=4.0",
        "wpackagist-plugin/wp-log-viewer": "1.0.7",
        "wpackagist-plugin/customizer-reset-by-wpzoom": "1.0.1",
        "phpunit/phpunit": "4.8.*"
    },
    "suggest": {
        "wpackagist-plugin/buoy": "Better Angels' Buoy is a community-based crisis response system."
    },
    "extra": {
        "installer-paths": {
            "./wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "./wp-content/themes/{$name}/": ["type:wordpress-theme"]
        }
    }
}
